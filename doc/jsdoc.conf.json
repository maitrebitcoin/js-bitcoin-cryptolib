{
    "_comment_command_line_to_use": "jsdoc -c jsdoc.conf.json",

    "plugins": [],
    "recurseDepth": 10,
    "source": {
        "include": ["../src/bitcoinwallet.js","../src/ecdsa.js"],
        "includePattern": ".+\\.js(doc|x)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "sourceType": "module",
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc","closure"]
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false
    }
}